{
  "name": "@witty-creatures/ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --outDir=../api/public",
    "build:test": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "start:api": "(cd ../api && yarn start) &",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "yarn build:test && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "yarn build:test && yarn start:api && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@hennge/vue3-pagination": "1.0.17",
    "@vueuse/core": "8.5.0",
    "axios": "0.27.2",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.2.2",
    "query-string": "7.1.1",
    "vue-barcode-reader": "1.0.1",
    "pinia": "2.0.13",
    "sass": "1.45.1",
    "vue": "3.2.33",
    "vue-router": "4.0.14",
    "web3": "1.7.3"
  },
  "devDependencies": {
    "@types/jsdom": "16.2.14",
    "@types/node": "16.11.34",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/test-utils": "2.0.0-rc.21",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.7",
    "cypress": "9.7.0",
    "jsdom": "19.0.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "unplugin-vue-components": "0.19.5",
    "vite": "2.9.9",
    "vitest": "0.12.9",
    "vue-tsc": "0.34.16"
  }
}
